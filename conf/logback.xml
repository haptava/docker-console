<configuration>

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>
        <!-- %d{HH:mm:ss.SSS} %-5level [%file:%line]--> - %msg [%thread]%n
      </pattern>
    </encoder>
  </appender>

  <!--<logger name="ch.qos.logback" level="WARN" />-->

  <root level="INFO">
    <appender-ref ref="STDOUT"/>
  </root>

</configuration>
